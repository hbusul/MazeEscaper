class ExtendedMath {

	//calculates a ^ b
	function int pow(int a, int b){
		var int temp, sqr, ctr;

		if(b = 0){
			return 1;
		}
		if(b = 1){
			return a;
		}

		if((b & 1) = 1){ //b is odd
			let b = b - 1;
			let temp = a;
		}else {
			let temp = 1;
		}

		let a = a * a;
		let b = b / 2;
		let ctr = 0;
		while(ctr < b){
			let temp = temp * a;
			let ctr = ctr + 1;
		}
		return temp;
	}


}